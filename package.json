{
  "name": "@ideagays/validator",
  "version": "0.0.11",
  "main": "lib/validator.common.js",
  "unpkg": "lib/validator.umd.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:ideagay/validator.git"
  },
  "keywords": [
    "validator",
    "form"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name validator ./src/index.js --dest lib",
    "lint": "vue-cli-service lint",
    "release": "bump --prompt --commit --tag --push --all"
  },
  "dependencies": {
    "vue": "^2.5.17"
  },
  "files": [
    "src",
    "lib"
  ],
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "version-bump-prompt": "^4.3.0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}